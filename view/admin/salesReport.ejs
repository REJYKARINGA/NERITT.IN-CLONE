                    document.getElementById('btnMonth').addEventListener('click', function() {
                        document.getElementById('filters').value = 'month';
                        submitForm();
                    });
                
                    // Weekly filter button click event
                    document.getElementById('btnWeek').addEventListener('click', function() {
                        document.getElementById('filters').value = 'weekly';
                        submitForm();
                    });
                
                    // Day filter button click event
                    document.getElementById('btnDayToday').addEventListener('click', function() {
                        document.getElementById('filters').value = 'today';
                        submitForm();
                    });
                
                    // Function to submit the form
                    function submitForm() {
                        document.querySelector('form').submit();
                    }
                </script>
                
                

