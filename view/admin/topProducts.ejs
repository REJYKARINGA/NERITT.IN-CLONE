            <tbody id="product_body">
                <% products.forEach((product,index)=>{ %>
                                    
                <tr>
                    <td>
                        <div class="td-content"><%=index+1%></div>
                    </td>
                    <td>
                        <div class="td-content"><%=product.sku%></div>
                    </td>
                    <td>
                        <div class="ProductTableImage"><img src='/<%=product.gallery[0]%>' class="img-fluid">
