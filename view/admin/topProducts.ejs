                    </td>
                    <!-- <td>
                                        <div class="td-content">SHIRTING &amp; GIRLS TOP</div>
                                    </td> -->
                    <td>
                        <div class="td-content"><%=product.price%></div>
                    </td>
                   
                    <td>
                        <div class="td-content"><%=topProducts[index].count%></div>
                    </td>
                    <!-- <td>
                        <div class="td-content"><% product.category[0].category_name %></div>
                    </td> -->
                   
                                        <td>
                        <div class="td-content"><%=product.sales_cost %></div>
                    </td>
                    <td>
                        <div class="td-content"><%=product.delivery_charge %></div>
                    </td>
                    <td>
                        <div class="td-content"><%=product.cgst%> %</div>
                    </td>
                    <td>
                        <div class="td-content"><%=product.sgst%> %</div>
                    </td>
                    <td><span for="status" class="badge bg-success"
                            style="color:white;"><%=product.stock_status%></span>
                    </td>
                    
                </tr>
                                                                     
                <% }); %>
     
                <%}%>           
