                function toggleLearnerTrainer() {

                    if (memberType == 'P' || enrollType == 'company') {
                        $("#cityField").hide();
                        $("#providerType").show();
                        $(".provider").show();
                        $(".locationField").show();
                        if (enrollType == 'INSTITUTE') {
                            $(".individ").hide();
                        }

                    } else {
                        $("#providerType").hide();
                        $(".provider").hide();
                        $('#name').show();
                        $("#cityField").hide();
                        $(".locationField").show();
                        $("#instituteName").hide();
                        $("#cityField").show();

                    }
                }

                function toggleIndividInfo() {

                    if (((typeof memberType !== 'undefined' && memberType == 'TRAINER') || enrollType == 'INSTITUTE') &&
                        providerType == 'INDIVIDUAL') {
                        $("#instituteName").hide();
                        $(".individ").show();
                        $('#name').show();
                    } else if ((typeof memberType !== 'undefined' && memberType == 'TRAINER') || enrollType ==
                        'INSTITUTE') {
                        $('#name').hide();
                        $("#instituteName").show();
                        $(".individ").hide();
                    }
                }
            });
