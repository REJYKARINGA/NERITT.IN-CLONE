            width: 80px;
            height: 80px;
            object-fit: cover;
            border-radius: 5px;
            margin-right: 10px;
        }

        .quantity-price {
            display: flex;
            align-items: center;
        }

        .totals {
            margin-bottom: 20px;
        }

        .total {
            font-weight: bold;
        }

        .payment-methods label {
            display: block;
            margin-bottom: 10px;
        }

        .btn {
            background-color: #007bff;
            color: #fff;
            padding: 10px 20px;
            text-decoration: none;
            border-radius: 5px;
            display: inline-block;
        }

        .btn:hover {
            background-color: #0056b3;
        }
