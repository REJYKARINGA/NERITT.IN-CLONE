<script>
    document.getElementById("myFormPassword").addEventListener("submit", function(event) {
        event.preventDefault(); // Prevent the form from submitting

        // Get the values entered by the user
        var password = document.getElementById("password").value;
        var confirmPassword = document.getElementById("password-confirm").value;

        // Regular expressions for strong password validation
        var uppercaseRegex = /[A-Z]/; // At least one uppercase letter
        var lowercaseRegex = /[a-z]/; // At least one lowercase letter
        var digitRegex = /[0-9]/; // At least one digit
        var specialCharRegex = /[!@#$%^&*()_+\-=\[\]{};':"\\|,.<>\/?]/; // At least one special character
        var minLength = 8; // Minimum password length

        // Check if password field is empty
        if (password.trim() === "") {
            document.getElementById("password_err").textContent = "Password is required";
            document.getElementById("password_err").style.display = "block";
            return false;
        } else if (password.length < minLength) {
            document.getElementById("password_err").textContent = "Password must be at least " + minLength + " characters long";
            document.getElementById("password_err").style.display = "block";
            return false;
        } else if (!uppercaseRegex.test(password)) {
            document.getElementById("password_err").textContent = "Password must contain at least one uppercase letter";
            document.getElementById("password_err").style.display = "block";
            return false;
        } else if (!lowercaseRegex.test(password)) {
            document.getElementById("password_err").textContent = "Password must contain at least one lowercase letter";
            document.getElementById("password_err").style.display = "block";
            return false;
        } else if (!digitRegex.test(password)) {
            document.getElementById("password_err").textContent = "Password must contain at least one digit";
            document.getElementById("password_err").style.display = "block";
            return false;
        } else if (!specialCharRegex.test(password)) {
            document.getElementById("password_err").textContent = "Password must contain at least one special character";
            document.getElementById("password_err").style.display = "block";
            return false;
        } else {
            document.getElementById("password_err").style.display = "none";
        }

        // Check if confirm password field is empty
        if (confirmPassword.trim() === "") {
            document.getElementById("cnfpassword_err").textContent = "Confirm Password is required";
            document.getElementById("cnfpassword_err").style.display = "block";
            return false;
        } else if (password !== confirmPassword) {
            document.getElementById("cnfpassword_err").textContent = "Passwords do not match";
            document.getElementById("cnfpassword_err").style.display = "block";
            return false;
        } else {
            document.getElementById("cnfpassword_err").style.display = "none";
        }

        // If validation passes, submit the form
        this.submit();
    });
</script>


